\n","            var c = ca[i];\n","            while (c.charAt(0)==' ') c = c.substring(1,c.length);\n","            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);\n","        }\n","        return null;\n","    }\n","\n"," </script>\n","<script type=\"application/ld+json\">        \n","{\n","\"@context\": \"https://schema.org\",\n","            \"@type\": \"WebPage\",\n","            \"name\": \"The top 10 richest people in India in 2024\",\n","            \"description\": \"Gain insights into the extraordinary wealth and remarkable achievements of the ten wealthiest individuals in India, spanning various industries\",\n","            \"url\" : \"https://www.forbesindia.com/article/explainers/top-10-richest-people-india/85909/1\",\n","            \"publisher\": {\n","                \"@type\": \"Organization\",\n","                \"name\": \"ForbesIndia\",\n","                \"url\" : \"https://www.forbesindia.com/\",\n","                \"logo\" : {\n","                        \"@type\": \"ImageObject\",\n","                        \"contentUrl\": \"https://www.forbesindia.com/forbes-rvp-files/images/fi-newlogo.jpg\"\n","                        }\n","            }\n","        } \n","    </script>\n","<script type=\"application/ld+json\">           \n","\t{\n","             \"@context\": \"https://schema.org\",\n","             \"@type\": \"BreadcrumbList\",\n","             \"itemListElement\":\n","             [\n","              {\n","               \"@type\": \"ListItem\",\n","               \"position\": 1,\n","               \"item\":\n","               {\n","                \"@id\": \"https://www.forbesindia.com/\",\n","                \"name\": \"Home\"\n","                }\n","            }, \n","                        {\n","                          \"@type\": \"ListItem\",\n","                          \"position\": 2,\n","                          \"item\":{\n","                            \"@id\": \"https://www.forbesindia.